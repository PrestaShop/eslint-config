{
  "name": "eslint-config-prestashop",
  "version": "0.2.1",
  "description": "PrestaShop's JS ESLint config, following our styleguide",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --report-unused-disable-directives .",
    "test": "npm run --silent tests-only",
    "tests-only": "babel-tape-runner ./tests/test-*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PrestaShop/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "styleguide"
  ],
  "author": "PrestaShop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PrestaShop/eslint-config/issues"
  },
  "homepage": "https://github.com/PrestaShop/eslint-config#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "babel-preset-airbnb": "^5.0.0",
    "babel-tape-runner": "^3.0.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "tape": "^5.3.2"
  },
  "peerDependencies": {
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3"
  }
}
